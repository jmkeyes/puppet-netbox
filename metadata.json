{
  "name": "jmkeyes-netbox",
  "version": "0.0.1",
  "author": "jmkeyes",
  "summary": "Module for managing DigitalOcean's Netbox.",
  "license": "Apache-2.0",
  "source": "https://github.com/jmkeyes/puppet-netbox",
  "project_page": "https://github.com/jmkeyes/puppet-netbox",
  "issues_url": "https://github.com/jmkeyes/puppet-netbox/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "jfryman/nginx",
      "version_requirement": ">= 0.3.0 < 1.0.0"
    },
    {
      "name": "stankevich/python",
      "version_requirement": ">= 1.14.0 < 2.0.0"
    }
  ]
}
